  "type": "module",
  "private": true,
  "workspaces": [
    "modules/*"
  ],
  "scripts": {
    "build:core": "yarn --cwd \"modules/core\" build",
    "build:flex": "yarn --cwd \"modules/flex\" build",
    "build:react": "yarn --cwd \"modules/react\" build",
    "serve": "es-dev-server --node-resolve --watch",
    "test:jest": "jest",
    "test": "karma start karma.conf.js",
    "test:watch": "karma start karma.conf.js --auto-watch=true --single-run=false"
  },
  "dependencies": {
    "@mantra-ui/core": "2021.0.0",
    "@mantra-ui/eslint-config": "2021.0.0",
    "@mantra-ui/flex": "2021.0.0",
    "@mantra-ui/react": "2021.0.0",
    "@open-wc/testing": "^2.5.32",
    "@open-wc/testing-karma": "^4.0.9",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.1.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@rollup/plugin-typescript": "^8.1.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@types/jest": "^26.0.20",
    "@typescript-eslint/parser": "^4.15.0",
    "deepmerge": "^4.2.2",
    "es-dev-server": "^2.1.0",
    "eslint": "^7.19.0",
    "jest": "^26.6.3",
    "lodash": "^4.17.20",
    "rollup": "^2.39.0",
    "rollup-plugin-delete": "^2.0.0",
    "rollup-plugin-dts": "^2.0.1",
    "rollup-plugin-filesize": "^9.1.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "sinon": "^9.2.4",
    "ts-jest": "^26.5.1",
    "typescript": "^4.1.3"
  }
}
